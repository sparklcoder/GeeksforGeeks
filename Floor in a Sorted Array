int findFloor(vector<long long> v, long long n, long long x){
        int start=0,end=n-1;
        while(start<=end){
            int mid=start+(end-start)/2;
            if(v[mid]==x){
                return mid;
            }
            else if(v[mid]<x){
                start=mid+1;
            }
            else{
                end=mid-1;
            }
        }
        return end;
        
    }
